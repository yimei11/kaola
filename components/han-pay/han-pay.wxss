/* components/han-pay/han-pay.wxss */
.seec-pay-mask {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0,0,0,.5);
  color: #000;
  z-index: 900;
}
.seec-pay-mask .pay-wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 901;
  transition: bottom .3s;
}
.seec-pay-mask.open .pay-wrapper {
  bottom: 405rpx;
}
.seec-pay-mask .pay {
  position: relative;
  width: 480rpx;
  height: 535rpx;
  border-radius: 15rpx;
  padding: 0 20px;
  background: #fff;
}
.seec-pay-mask .close {
  position: absolute;
  top: 0;
  left: 0;
  width: 100rpx;
  height: 100rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 28rpx 28rpx;
}
.seec-pay-mask .title {
  line-height: 100rpx;
  font-size: 32rpx;
  text-align: center;
}
.seec-pay-mask .name {
  line-height: 60rpx;
  font-size: 32rpx;
  text-align: center;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.seec-pay-mask .total {
  line-height: 100rpx;
  font-size: 64rpx;
  font-weight: bold;
  text-align: center;
}
.seec-pay-mask .type {
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-top: 1px solid #ededed;
  height: 90rpx;
  margin: 15px 0;
  font-size: 28rpx;
}
.seec-pay-mask .type .label {
  color: #777;
}
.seec-pay-mask .type .icon {
  padding-left: 45rpx;
  background: url(data:image/png;base64,) no-repeat left center;
  background-size: 36rpx 36rpx;
}
.seec-pay-mask .input {
  display: flex;
  border: 1px solid #ededed;
  height: 84rpx;
}
.seec-pay-mask .input .view {
  position: relative;
  flex: 1;
  box-sizing: border-box;
  line-height: 84rpx;
  text-align: center;
  font-size: 32rpx;
}
.seec-pay-mask .input .view + .view {
  border-left: 1px solid #ededed;
}
.seec-pay-mask .input .view:after {
  content: "";
  position: absolute;
  top: 50%;
  left: 50;
  margin: -11rpx 0 0 -11rpx;
  width: 22rpx;
  height: 22rpx;
  border-radius: 50%;
  background: #222;
  opacity: 0;
  transition: .2s;
}
.seec-pay-mask .input .view.active:after {
  opacity: 1;
}

.seec-pay-mask .keyboard {
  display: flex;
  flex-wrap: wrap;
  position: fixed;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 902;
  background: #ededed;
  border-top: 1px solid #ededed;
  transform: translateY(100%);
  transition: .3s;
}
.seec-pay-mask.open .keyboard {
  transform: translateY(0);
}
.seec-pay-mask .keyboard .view {
  box-sizing: border-box;
  width: 33.333%;
  line-height: 100rpx;
  border-right: 1px solid #ededed;
  border-bottom: 1px solid #ededed;
  background: #fff;
  text-align: center;
  font-size: 40rpx;
  font-weight: bold;
  color: #222;
}
.seec-pay-mask .keyboard .view:nth-child(3n) {
  border-right-width: 0;
}
.seec-pay-mask .keyboard .view:nth-child(10) {
  background: #ededed;
}
.seec-pay-mask .keyboard .view:nth-child(12) {
  background: #ededed url(data:image/png;base64,) no-repeat center center;
  background-size: 38rpx auto;
}

.seec-pay-mask .success {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 903;
  background: #fff;
}
.seec-pay-mask .success .h2 {
  margin-top: 5%;
  background: url(data:image/png;base64,) no-repeat left center;
  background-size: 36rpx auto;
  padding-left: 40rpx;
  font-size: 36rpx;
  color: #57be6a;
}
.seec-pay-mask .success .h3 {
  margin-top: 20%;
  font-size: 36rpx;
}
.seec-pay-mask .success .h4 {
  font-size: 65rpx;
}
.seec-pay-mask .success .button {
  margin-top: 85%;
  width: 334rpx;
  height: 74rpx;
  line-height: 74rpx;
  border-radius: 10rpx;
  background: #f2f2f2;
  color: #3ca450;
  font-size: 36rpx;
  text-align: center;
}
