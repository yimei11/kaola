page {
  background-color: #F0F0F0;
  font-size: 24rpx;
  color: #000;
}
.nologin .top {
  padding: 0 30rpx;
  height: 300rpx;
  background-color: #fff;
  border-radius: 0 0 200rpx 200rpx / 0 0 40rpx 40rpx;
  overflow: hidden;
}
.nologin .top .login_register {
  font-size: 48rpx;
  font-weight: 500;
  padding: 30rpx 0;
}
.nologin .top .text {
  font-size: 28rpx;
  font-weight: 420;
}
.nologin .fk {
  margin: 30rpx;
}
.nologin .yh {
  margin: 0 30rpx;
}
.nologin .help {
  margin: 30rpx 30rpx;
  padding: 30rpx;
  background-color: #fff;
  display: flex;
}
.nologin .help .item {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-right: 30rpx;
}
.nologin .help .item .img {
  width: 60rpx;
  height: 60rpx;
  padding: 10rpx;
  border-radius: 50%;
  background-color: #ccc;
}
.nologin .help .item .img image {
  width: 100%;
  height: 100%;
}
.nologin .help .item .text {
  margin-top: 10rpx;
}
.nologin .products {
  padding: 0 20rpx;
  margin-top: 50rpx;
}
.nologin .products .item {
  border-radius: 10rpx;
  overflow: hidden;
}
.nologin .products .van-grid-item__content--center {
  align-items: flex-start;
}
.nologin .products .img {
  width: 100%;
  height: 300rpx;
}
.nologin .products .img image {
  width: 100%;
  height: 100%;
}
.nologin .products .top-title {
  display: flex;
  align-items: center;
  margin: 10rpx 0;
}
.nologin .products .top-title .img {
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}
.nologin .products .top-title .img image {
  width: 100%;
  height: 100%;
}
.nologin .products .lang-title {
  margin: 10rpx 0;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.nologin .products .lang-title .l-t-box {
  display: inline-block;
  background-color: red;
  border-radius: 20rpx;
  padding: 0 10rpx;
  color: #fff;
  margin-right: 10rpx;
}
.nologin .products .price {
  color: #f00;
  font-size: 32rpx;
  font-weight: 600;
}
.nologin .products .price .price-t {
  font-weight: 400;
  font-size: 24rpx;
}
.dialog .text {
  padding: 30rpx;
  text-align: center;
}
